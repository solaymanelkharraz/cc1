{
    "version": 2,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.2" }
    },
    "routes": [
        { "src": "/(css|js|img|fonts)/(.*)", "dest": "/public/$1/$2" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "env": {
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "CACHE_DRIVER": "array"
    }
}